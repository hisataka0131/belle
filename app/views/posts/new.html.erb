<h1>新規投稿</h1>
<%= form_for(@post) do |f| %>
 
  <h4>タイトル</h4>
  <%= f.text_field :post_name %>
 
  <h4>本文</h4>
  <%= f.text_area :post_opinion %>

  <h4>カテゴリー</h4>
  <%= f.select :category, Post.categories.keys.to_a, {} %>
 

 
 
  <h4>画像</h4>
  <%= f.attachment_field :post_image %>
 
  <%= f.submit '投稿' %>
 
<% end %>
